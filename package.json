{
  "name": "vue3-webdiary",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "vite": "vite",
    "mock": "json-server --watch ./mock/data.json --port 4000",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
