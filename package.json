    "md-editor-v3": "^1.11.9",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "prismjs": "^1.27.0",
    "qs": "^6.10.3",
    "showdown": "^2.1.0",
    "tinymce": "^6.0.1",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.9",
    "@types/node": "^17.0.24",
    "@types/qs": "^6.9.7",
    "@types/showdown": "^2.0.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "typescript": "^4.5.4",
